{
  "Logging": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "System": "Warning",
        "Microsoft": "Warning"
      }
    }
  },
  "Kafka": {
    "group.id": "test",
    "Consumer": {
      "enable.auto.commit": true,
      "enable.auto.offset.store": false
    },
    "Message": {
      "Default": "offset: storedOrEnd, buffer(3), retry(), store()",
      "Sample": "dateOffset: 2020-01-01"
      //"Test": "topic = event.payments.deposit.changed-STAGE, offset(end, -100), buffer(200)"
    },
    "Placeholders": {
      "env": "REPLACED"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
  }
}
