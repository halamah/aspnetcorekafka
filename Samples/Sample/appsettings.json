{
  "Logging": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "System": "Warning",
        "Microsoft": "Warning"
      }
    }
  },
  "Kafka": {
    "Consumer": {
      "enable.auto.commit": false
    },
    "Message": {
      "Default": "offset: storedOrEnd, buffer(101), commit(), options(skipNullMessages, retryFailure)",
      "Test": "topic = event.payments.deposit.changed-STAGE, offset(end, -100), buffer(200)"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
  }
}
